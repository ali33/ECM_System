                    <div style="float:right">
                        <button type="button" id="build_execute_command" class="archive_button dialog_button" style="float:right">
                            <span>Build Command</span>
                            <img src="~/Images/button/appbar.add.32.png" />
                        </button>
                    </div>
                </fieldset>
            </div>

            <fieldset class="dialog_fieldset" id="text_sql">
                <legend>Sql command</legend>
                <div style="float:left; width:400px;height:100px">
                    <textarea name="text_area_condition" id="sql_command" class="dialog_textarea"></textarea>
                </div>
                <div style="float:right">
                    <button type="button" id="test_lookup_data" class="archive_button dialog_button">
                        <span>Test SQL</span>
                        <img src="~/Images/button/uploader.png" />
