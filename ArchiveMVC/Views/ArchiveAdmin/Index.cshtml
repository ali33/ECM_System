                        <select id="select_database_field" class="dialog_select">
                        </select>
                    </div>
                    <div class="div_content">
                        <div class="dialog_label">Operator</div>
                        <select id="select_operator" class="dialog_select">
                            <option>Empty</option>
                        </select>
                    </div>
                    <div class="div_content" style="margin: 10px 0px -15px 0px;">
                        <label class="dialog_label"></label>
                        <button type="button" id="button_add_condition" class="archive_button dialog_button">
                            <span>Add Condition</span>
                            <img src="~/Images/button/appbar.add.32.png" />
                        </button>
                        <button type="button" id="button_clear_condition" class="archive_button dialog_button">
                            <span>Clear Condition</span>
                            <img src="~/Images/button/appbar.reset.32.png" />
                        </button>
                    </div>
                </fieldset>
            </div>
            <div id="commandstore" style="display:none">
                <fieldset class="dialog_fieldset">
                    <legend>Lookup data</legend>
                    <div class="div_content">
                        <div class="dialog_label">Database field</div>
                        <select id="select_database_field_stored" class="dialog_select">
                        </select>
                    </div>
                    <div class="div_content">
                        <div class="dialog_label">Operator</div>
                        <select id="select_operator_stored" class="dialog_select">
