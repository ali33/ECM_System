<section class="body archive body_archive_admin">
    <div class="left_column archive_admin_menu">
        <div class="admin_menu_title">
            <div class="title_content">Admin Menu</div>
        </div>
        <div id="content_types" class="admin_menu_item selected">
            <div class="icon">
                <img src="@Url.Content("~/Images/archive_admin_icon/appbar.debug.step.into.svg")" />
            </div>
            <div class="item_content">
                <div class="item_content_data">Content types</div>    
            </div>
        </div>

        <div id ="permissions" class="admin_menu_item">
            <div class="icon">
                <img src="@Url.Content("~/Images/archive_admin_icon/appbar.key.svg")" />
            </div>
            <div class="item_content">
                <div class="item_content_data">Permissions</div> 
            </div>
        </div>
        
        <div id="users" class="admin_menu_item">
            <div class="icon">
                <img src="@Url.Content("~/Images/archive_admin_icon/appbar.user.minus.svg")" />
            </div>
            <div class="item_content">
                <div class="item_content_data">Users</div>
            </div>
        </div>
        
        <div id="user_groups" class="admin_menu_item">
            <div class="icon">
                <img src="@Url.Content("~/Images/archive_admin_icon/appbar.group.svg")" />
            </div>
            <div class="item_content">
                <div class="item_content_data">User Groups</div>
            </div>
        </div>
        
        <div id="settings" class="admin_menu_item">
            <div class="icon">
                <img src="@Url.Content("~/Images/archive_admin_icon/appbar.settings.svg")" />
            </div>
            <div class="item_content">
                <div class="item_content_data">Settings</div>
            </div>
        </div>

    </div>
    <div class="right_column archive_admin_container">
        <div class="admin_sub_menu"></div>
        <div class="sub_properties"></div>
    </div>
    <div class="ocr_template_panel"></div>
</section>

<div id="dialog_add_field" style="display:none;">

    <fieldset class="dialog_fieldset" style="height: 120px;">
        <legend>Fields</legend>
        <div class="div_content">
            <div class="dialog_label">Field name</div>
            <input type="text" id="field_name" class="archive_input dialog_input" placeholder="Enter field name...">
        </div>    
        
        <div class="div_content">
            <div class="dialog_label">Data type</div>
            <select name="data_type" id="dataType" class="dialog_select">
            @foreach (Ecm.Domain.FieldDataType dataType in Enum.GetValues(typeof(Ecm.Domain.FieldDataType)))
            {
                <option value="@dataType.ToString()">@dataType.ToString()</option>
            }                
            </select>
        </div>
        
    </fieldset>

    <fieldset class="dialog_fieldset" style="height: 200px;">
        <legend>Fields configure</legend>

        <div class="div_content" style="height:25px;">
            <div class="div_content_other">
                <input id="required" type="checkbox" value="fasle" /> Is required                
                <input id="restricted" type="checkbox" value="false" /> Is restricted
            </div>

        </div>
        <div class="div_content">
            <div class="div_content_button">
                <div class="hidden archive_button dialog_button" id="configure_table" style="display: none">
                    <span style="width: 70px; margin-top:5px">Configure column</span>
                </div>
            </div>
            
            <div  class="div_content_button">
                <div class="hidden archive_button dialog_button" id="configure_picklist" style="display: none">
                    <span style="width: 70px; margin-top:5px">Configure picklist</span>
                </div>
            </div>
        </div>
        <div class="div_content">
            <div class="dialog_label">Max length</div>
            <input type="text" name="max_length" id="max_length" class="archive_input dialog_input" value="0"/>
        </div>

        <div class="div_content">
            <div class="dialog_label">Default value</div>
            @* ------------ *@
            <input type="text" name="default_value" id="default_value" class="archive_input dialog_input" />
            @* ------------ *@
            <div class="div_content_other current_date" style="display: none;">
                <input id="use_current_date" type="checkbox" value="false"/> Use current date   
            </div>
            @* ------------ *@
            <select id="select_true_false" class="dialog_select" style="display: none;">
                <option value="True">True</option>  
                <option value="Fasle">Fasle</option>                        
            </select>
        </div>
    </fieldset>

</div>
<!-- Dialog picklist -->
<div id="picklist_value_dialog" style="display: none;">
  <fieldset class="dialog_fieldset">
      <legend>Picklist value</legend>
      <textarea name="picklist_value" id="picklist_value" cols="25" rows="10"></textarea>
  </fieldset>
  
 
</div>
<!-- Dialog configure table -->
<div id="dialog_table_configure" style="display: none;">
  @*<h2>Table columns configuration</h2>*@
    <div class="table_dialog_configuration">
        <div class="table">
            <table id="table_configuration" class="archive_table tablesorter">
                <thead>
                    <tr>
                        <th>&nbsp;</th>
                        <th>Column name</th>
                        <th>Data type</th>
                        <th>Max length</th>
                        <th>Default value</th>
                    </tr>
                </thead>
                <tbody>
                    <tr class="table_configure_row first_row">
                        <td id="first_delete_column">
                            <img class="dialog_table_icon" src="~/Resources/table_configure_delete_icon.png">
                        </td>
                        <td class="table_td_name">
                            <input type="text" class="archive_input dialog_table_input table_name_value" placeholder="New column">
                        @*</td>*@
                        <td class="table_td_type">
                            <select class="archive_select table_data_type">
                                <option value="String" selected>String</option>
                                <option value="Integer">Integer</option>
                                <option value="Decimal">Decimal</option>
                                <option value="Date">Date</option>
                            </select>
                        </td>
                        <td class="table_td_maxlength">
                            <input type="text" class="archive_input dialog_table_input table_maxlength" value="0">
                        </td>
                        <td class="table_td_default_value">
                            <input type="text" class="archive_input dialog_table_input table_default_value">
                            <div class="dialog_table_checkbox table_use_current_date_contain hidden">
                                <input type="checkbox" class="table_check_use_current_date" value="false"/>
                                <a>Use current date</a>
                            </div>
                        </td>
                    </tr>
                </tbody>
            </table>
        </div>
        <div class="add_conlumn_button">
            <div id="add_new_column" class="archive_button dialog_button add_new_column">
                <span>Click here to add a new column</span>
            </div>
        </div>

    </div>
    
</div>
<!-- Dialog configure table -->

<div id="dialog_lookup" style="display: none;">
    <div id="dialog_lookup_content">
        <ul>
              <li><a href="#tabs-ServerInfo">Server info</a></li>
              <li><a href="#tabs-MappingInfo">Mapping info</a></li>
              <li><a href="#tabs-CommandInfo" id="commandInfo">Command info</a></li>
        </ul>
        <div id="tabs-ServerInfo" class="tab_padding">
            <fieldset class="dialog_fieldset" style="height: 450px">
                <legend>Data sources</legend>
                <div class="div_content">
                    <div class="dialog_label">Server name</div>
                    <input type="text" id="server_name" class="archive_input dialog_input" placeholder="Enter server name...">
                </div>

                <div class="div_content">
                    <div class="dialog_label">Database type</div>
                    <select id="database_type_select" class="dialog_select">
                        <option value="0">MS SQL Server</option>
                        <option value="2">Oracle Database</option>
                        <option value="4">IBM DB2</option>
                        <option value="1">My SQL</option>
                        <option value="3">Postgre SQL</option>
                    </select>
                </div>
                
                <div class="div_content">
                    <div class="div_content">Data provider</div>
                    <select id="data_provider_select" class="dialog_select">
                        <option value="2">ADO.NET provider</option>
                        <option value="1">Oledb provider</option>
                    </select>
                </div>

                <div class="div_content">
                    <div class="dialog_label">Port number</div>
                    <input type="text" id="port_number" class="archive_input dialog_input" placeholder="Enter port number...">
                </div>

                <div class="div_content">
                    <div class="dialog_label">User name</div>
                    <input type="text" id="user_name" class="archive_input dialog_input" placeholder="Enter user name...">
                </div>

                <div class="div_content">
                    <div class="dialog_label">Password</div>
                    <input type="password" id="pass_word" class="archive_input dialog_input">
                </div>

                <div class="div_content">
                    <div class="dialog_label">Database</div>
                    <select id="select_database" class="dialog_select">
                        <option>Empty</option>
                    </select>
                </div>

                <div class="div_content">
                    <div class="dialog_label">Schema</div>
                    <select id="select_schema" class="dialog_select">
                        <option>Empty</option>
                    </select>
                </div>

                <div class="div_content">
                    <button type="button" id="button_test_connection" class="archive_button dialog_button">
                        <span>Test connection</span>
                        <img src="~/Images/button/uploader.png" />
                    </button>
                </div>

            </fieldset>
        </div>

        <div id="tabs-MappingInfo" class="tab_padding">
            <fieldset class="dialog_fieldset" style="height: 450px">
                <legend>Mapping info</legend>
                <div class="div_content">
                    <div class="dialog_label">Select data source type</div>
                    <input type="radio" id="display_table" name="data_source" value="Table" checked="checked">Display table
                    <input type="radio" id="display_view" name="data_source" value="View">Display views
                    <input type="radio" id="display_procedure" name="data_source" value="StoredProcedure">Display stored procedures
                </div>
                <div class="div_content">
                    <div class="dialog_label">Data source</div>
                    <select class="dialog_select" id="select_data_source">
                        <option id="Empty">Empty</option>
                    </select>
                </div>
                <div class="div_content">
                    <div class="dialog_label">Maximum number of lookup rows returned</div>
                    <input type="text" id="number_of_lookup_rows" class="archive_input dialog_input"/>
                </div>
                <div class="contain_table_look_data">
                    <table id="table_look_data" class="archive_table tablesorter">
                        <thead>
                            <tr>
                                <th>Archive client</th>
                                <th>Lookup data source</th>
                            </tr>                    
                        </thead>
                        <tbody>
                            
                        </tbody>
                    </table>
                </div>
            </fieldset>
        </div>

        <div id="tabs-CommandInfo" class="tab_padding">
            <div id="commandtext">            
                <fieldset class="dialog_fieldset">
                    <legend>Build Sql command</legend>
                    <div class="div_content">
