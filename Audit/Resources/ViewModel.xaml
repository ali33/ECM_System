<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:v="clr-namespace:Ecm.Audit.View"
                    xmlns:vm="clr-namespace:Ecm.Audit.ViewModel">
    <DataTemplate DataType="{x:Type vm:ActionLogViewModel}">
        <v:ActionLogView x:Name="LogView"/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:ReportViewModel}">
        <v:AuditView x:Name="AuditView"/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:AboutViewModel}">
        <v:AboutView x:Name="AboutView"/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:HistoryViewModel}">
        <v:HistoryView x:Name="HistoryView"/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:DeletedDocumentHistoryViewModel}">
        <v:DeletedDocumentHistoryView x:Name="DeletedDocumentHistoryView"/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:HistoryDetailViewModel}">
        <v:HistoryDetailView x:Name="HistoryDetailView"/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:DocumentViewModel}">
        <v:DocumentView x:Name="DocumentView"/>
    </DataTemplate>
    
</ResourceDictionary>