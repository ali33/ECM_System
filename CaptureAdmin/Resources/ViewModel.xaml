<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:Ecm.CaptureAdmin.ViewModel"
                    xmlns:v="clr-namespace:Ecm.CaptureAdmin.View">
    <DataTemplate DataType="{x:Type vm:UserViewModel}">
        <v:UserView x:Name="UserViewData"/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:UserGroupViewModel}">
        <v:UserGroupView x:Name="UserGroupViewData"/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:AboutViewModel}">
        <v:AboutView x:Name="AboutView"/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:PermissionViewModel}">
        <v:PermissionView x:Name="PermissionView"/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:BatchTypeViewModel}">
        <v:BatchTypeView x:Name="BatchTypeView"/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:HelpViewModel}">
        <v:HelpView x:Name="HelpView"/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:SettingViewModel}">
        <v:SettingView x:Name="SettingView"/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:ActionLogViewModel}">
        <v:ActionLogView x:Name="ActionLogView"/>
    </DataTemplate>
</ResourceDictionary>