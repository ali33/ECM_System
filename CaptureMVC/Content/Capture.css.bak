.rotate-90, .rotate--270 {
    -moz-transform: rotate(90deg);
    -ms-transform: rotate(90deg);
    -o-transform: rotate(90deg);
    -webkit-transform: rotate(90deg);
    transform: rotate(90deg);
}

.rotate-180, .rotate--180 {
    -moz-transform: rotate(180deg);
    -ms-transform: rotate(180deg);
    -o-transform: rotate(180deg);
    -webkit-transform: rotate(180deg);
    transform: rotate(180deg);
}

.rotate-270, .rotate--90 {
    -moz-transform: rotate(270deg);
    -ms-transform: rotate(270deg);
    -o-transform: rotate(270deg);
    -webkit-transform: rotate(270deg);
    transform: rotate(270deg);
    -moz-transform-origin: inherit;
    -ms-transform-origin: inherit;
    -o-transform-origin: inherit;
    -webkit-transform-origin: inherit;
    transform-origin: inherit;
}

.wrapper-thumbnail {
    box-shadow: 3px 3px 2px #888888;
    border: 1px solid #A19A9A;
    margin-right: 5px;
}

.wrapper-image, .wrapper-image-native-file {
    position: relative;
    /*padding: 5px;*/
    margin: 5px auto 10px;
    box-shadow: 3px 3px 2px #888888;
    border: 1px solid #A19A9A;
}

    .wrapper-image > image, .wrapper-image-native-file > image {
        display: block;
        border: none;
        margin: 0px;
        box-shadow: none;
    }

.treeview_select.capture_root,
.treeview_select.folder,
.treeview_select.page {
    box-shadow: 0 0 0 4px #00BFBF inset;
}

.ui-state-highlight {
    /*height: 1.5em;
    line-height: 1.2em;*/
}

.view-select {
}

.view-non-select {
    display: none;
}

.right_column {
    border: 1px solid #A19A9A;
}


.anno-drawing {
    background: none repeat scroll 0 0 rgba(0, 0, 0, 0);
    border: 1px dashed blue;
    position: absolute;
    -moz-box-sizing: border-box;
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
}

.anno-editing {
    cursor: text;
    box-shadow: 0 0 2px 2px blue;
}

.anno-retrict:hover {
    cursor: not-allowed;
    box-shadow: 0 0 2px 2px blue;
}

.anno_comment_inner {
    overflow: hidden;
    -ms-word-wrap: break-word;
    word-wrap: break-word;
}

.anno-select:hover, .anno-active {
    cursor: move;
    box-shadow: 0 0 2px 2px blue;
}

.cursor-pan-hover {
    cursor: url(../Images/Cursor/openhand.cur) !important;
    cursor: -moz-grab !important;
    cursor: -webkit-grab !important;
}

.cursor-pan-down {
    cursor: url(../Images/Cursor/closedhand.cur) !important;
    cursor: -moz-grabbing !important;
    cursor: -webkit-grabbing !important;
}

.cursor-draw {
    cursor: crosshair;
}

.cursor-select {
    cursor: move;
}

.user-select-text {
    -moz-user-select: text;
    -ms-user-select: text;
    -webkit-user-select: text;
    user-select: text;
}



/* Style for mandatory input */
.input-control.select.mandatory {
    border-color: none !important;
    box-shadow: none !important;
}

    .input-control.select.mandatory > select,
    .input-control.select.mandatory > select:focus {
        border: 1px solid rgba(192, 57, 43, 0.7);
        box-shadow: 0 0 3px 2px rgba(192, 57, 43, 0.4);
    }

.input-control.text.mandatory.focus {
    border-color: rgba(192, 57, 43, 0.7) !important;
    box-shadow: 0 0 3px 2px rgba(192, 57, 43, 0.4) !important;
}

/* Style for required label input */
.content_lables.required:after,
input-control.required:after {
    color: #C0392B;
    content: "*";
}
