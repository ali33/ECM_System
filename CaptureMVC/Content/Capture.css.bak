/* capture_sitebar capture_leftbar */

.capture_leftbar_ecm {
    overflow: hidden;
}

    .capture_leftbar_ecm > div {
        overflow: scroll;
    }

.leftbar_lable {
    position: relative;
    display: inline-block;
    /*width: 100%;*/
    /*height: 120px;*/
    margin-bottom: 2px;
    background-color: rgb(204, 204, 204);
    cursor: pointer;
    /*padding-right: 5px;*/
}

    .leftbar_lable > div {
        white-space: nowrap;
        display: inline-block;
        -moz-transform-origin: 15px 10px;
        -ms-transform-origin: 15px 10px;
        -o-transform-origin: 15px 10px;
        -webkit-transform-origin: 15px 10px;
        transform-origin: 15px 10px;
    }

    .leftbar_lable:hover,
    .leftbar_lable_focus {
        background-color: rgb(22, 160, 133);
        /*box-shadow: 0 0 0 1px rgba(51,102,102, 0.6);
        -moz-box-shadow: 0 0 0 1px rgba(51,102,102, 0.6);
        -webkit-box-shadow: 0 0 0 1px rgba(51,102,102, 0.6);*/
    }

.capture_sitebar .capture_leftbar {
    display: inline-block;
    position: relative;
    margin-bottom: 0;
    margin-left: 1px;
    margin-right: 1px;
    padding: 0;
    overflow-y: hidden;
    width: 30px;
    transition: width 0.2s linear;
    -moz-transition: width 0.2s linear;
    -o-transition: width 0.2s linear;
    -webkit-transition: width 0.2s linear;
}

    .capture_sitebar .capture_leftbar .leftbar_lable {
        position: relative;
        display: block;
        width: 100%;
        height: 120px;
        margin-bottom: 5px;
        background-color: rgb(204, 204, 204);
        cursor: pointer;
    }

        .capture_sitebar .capture_leftbar .leftbar_lable.leftbar_lable_focus {
            background-color: rgb(22, 160, 133);
            box-shadow: 0 0 0 1px rgba(51,102,102, 0.8);
            -moz-box-shadow: 0 0 0 1px rgba(51,102,102, 0.8);
            -webkit-box-shadow: 0 0 0 1px rgba(51,102,102, 0.8);
        }

        .capture_sitebar .capture_leftbar .leftbar_lable:hover {
            box-shadow: 0 0 0 1px rgba(51,102,102, 0.6);
            -moz-box-shadow: 0 0 0 1px rgba(51,102,102, 0.6);
            -webkit-box-shadow: 0 0 0 1px rgba(51,102,102, 0.6);
        }

        .capture_sitebar .capture_leftbar .leftbar_lable img {
            display: inline-block;
            position: absolute;
            width: 24px;
            height: 24px;
            margin-left: 3px;
        }

        .capture_sitebar .capture_leftbar .leftbar_lable span {
            display: block;
            float: left;
            font-size: 15px;
            margin-left: 1px;
            margin-right: 2px;
            margin-top: 27px;
            position: absolute;
            white-space: nowrap;
            width: 30px;
        }

        .capture_sitebar .capture_leftbar .leftbar_lable.thumbnails {
            margin-top: 60px;
        }

            .capture_sitebar .capture_leftbar .leftbar_lable.thumbnails span {
                background-image: url(../Images/thumbnails.png);
                height: 82px;
            }

        .capture_sitebar .capture_leftbar .leftbar_lable.indexs span {
            background-image: url(../Images/indexs.png);
            height: 53px;
        }

/* nav_resize */

div.capture_sitebar .nav_resize {
    display: inline-block;
    position: absolute;
    clear: both;
    width: 24px;
    height: 24px;
    top: 2px;
    right: 2px;
    background-image: url(../Images/icon_map.png);
    background-position: -6px -90px;
    opacity: .5;
    z-index: 999;
    transition: transform 0.3s linear 0.2s;
    -moz-transition: -moz-transform 0.3s linear 0.2s;
    -o-transition: -o-transform 0.3s linear 0.2s;
    -webkit-transition: -webkit-transform 0.3s linear 0.2s;
    cursor: pointer;
}

div.capture_sitebar.open .nav_resize {
    transform: none;
    -moz-transform: none;
    -ms-transform: none;
    -o-transform: none;
    -webkit-transform: none;
}

div.capture_sitebar.collapse .nav_resize {
    transform: rotate(180deg);
    -moz-transform: rotate(180deg);
    -ms-transform: rotate(180deg);
    -o-transform: rotate(180deg);
    -webkit-transform: rotate(180deg);
}

div.capture_sitebar .nav_resize:hover {
    opacity: 1;
}

/* capture_sitebar capture_mainbar */

.capture_sitebar .capture_mainbar {
    display: inline-block;
    position: absolute;
    margin: 0;
    padding: 0;
    padding-left: 2px;
    top: 0;
    /*max-width: 300px;*/
    width: 90%;
    height: 100%;
}

    /* capture header */

    .capture_sitebar .capture_mainbar .capture_header {
        position: relative;
        display: block;
        width: 100%;
        height: 60px;
        cursor: default;
    }

.capture_sitebar.type_collapse .capture_mainbar .capture_header {
    overflow: hidden;
}

.capture_header .capture_header_feature {
    position: relative;
    display: inline-block;
    width: 56px;
    height: 50px;
    margin-right: 7px;
    margin-left: 2px;
    margin-top: 4px;
    margin-bottom: 6px;
    background-color: rgba(51,102,102, 0.4);
    box-shadow: 0 2px 0 rgba(0,0,0,0.1);
    -moz-box-shadow: 0 2px 0 rgba(0,0,0,0.1);
    -webkit-box-shadow: 0 2px 0 rgba(0,0,0,0.1);
}

    .capture_header .capture_header_feature:hover {
        background-color: rgb(22, 160, 133);
        box-shadow: 0 0 0 1px rgba(51,102,102, 0.6), 0 2px 0 rgba(0,0,0,0.1);
        -moz-box-shadow: 0 0 0 1px rgba(51,102,102, 0.6), 0 2px 0 rgba(0,0,0,0.1);
        -webkit-box-shadow: 0 0 0 1px rgba(51,102,102, 0.6), 0 2px 0 rgba(0,0,0,0.1);
    }

        .capture_header .capture_header_feature:hover .span_show {
            color: rgba(0,0,0, .6);
        }

    .capture_header .capture_header_feature span.bt_images {
        display: inline-block;
        position: absolute;
        height: 40px;
        width: 40px;
        margin-top: 5px;
        margin-bottom: 5px;
        background-position: center;
        background-repeat: no-repeat;
    }

    .capture_header .capture_header_feature.scan_feature span.bt_images {
        background-image: url(../Images/icon_map.png);
        background-position: -5px -116px;
        background-repeat: no-repeat;
    }

    .capture_header .capture_header_feature.upload_feature span.bt_images {
        background-image: url(../Images/icon_map.png);
        background-position: -5px -166px;
        background-repeat: no-repeat;
    }

    .capture_header .capture_header_feature.webcam_feature span.bt_images {
        background-image: url(../Images/icon_map.png);
        background-position: -5px -216px;
        background-repeat: no-repeat;
    }

    .capture_header .capture_header_feature div.span_show {
        position: relative;
        top: 1px;
        display: inline-block;
        font-family: 'iconFont';
        -webkit-font-smoothing: antialiased;
        font-style: normal;
        font-weight: normal;
        line-height: 1;
    }

        .capture_header .capture_header_feature div.span_show:before {
            font-size: 14px;
            content: "\e10a";
        }

    .capture_header .capture_header_feature div.span_show {
        display: inline;
        position: absolute;
        top: 15px;
        right: 2px;
        color: #fff;
        font-size: 12px;
        transform: rotate(90deg);
        -moz-transform: rotate(90deg);
        -o-transform: rotate(90deg);
        -webkit-transform: rotate(90deg);
    }

        /* capture-dropdown-menu */

        .capture_header .capture_header_feature div.span_show .capture-dropdown-menu {
            position: absolute;
            display: none;
            background-color: #fff;
            margin-left: 0;
            list-style: none;
            top: 35px;
            left: -41px;
            z-index: 11010;
            float: left;
            border: 1px solid rgba(0, 0, 0, 0.2);
            box-shadow: 0 5px 10px rgba(0, 0, 0, 0.2);
            -moz-box-shadow: 0 5px 10px rgba(0, 0, 0, 0.2);
            -webkit-box-shadow: 0 5px 10px rgba(0, 0, 0, 0.2);
            min-width: 160px;
            padding-bottom: 5px;
            padding-top: 5px;
            padding-left: 0;
        }

            .capture_header .capture_header_feature div.span_show .capture-dropdown-menu.open {
                display: block;
                margin-bottom: 15px;
                margin-left: 0px;
            }

            .capture_header .capture_header_feature div.span_show .capture-dropdown-menu li {
                display: list-item;
                line-height: 20px;
            }

                .capture_header .capture_header_feature div.span_show .capture-dropdown-menu li:hover {
                    background: rgba(51,102,102,0.4);
                }

                .capture_header .capture_header_feature div.span_show .capture-dropdown-menu li a {
                    font-family: 'Segoe UI', 'Open Sans', Verdana, Arial, Helvetica, sans-serif;
                    font-weight: 400;
                    font-size: 11pt;
                    letter-spacing: 0.01em;
                    line-height: 13pt;
                    -webkit-font-smooth: always;
                    color: #000000;
                    display: block;
                    width: 100%;
                    padding: 3px 20px;
                    white-space: nowrap;
                    font-size: 14px;
                    cursor: pointer;
                    text-decoration: none;
                }

